<script type='text/javascript'>
$(function() {
 Dashing.gridsterLayout('[{"col":4,"row":1,"size_x":1,"size_y":1},{"col":1,"row":1,"size_x":1,"size_y":2},{"col":2,"row":1,"size_x":2,"size_y":1},{"col":2,"row":2,"size_x":2,"size_y":1},{"col":3,"row":3,"size_x":2,"size_y":1},{"col":4,"row":2,"size_x":1,"size_y":1}]')  
