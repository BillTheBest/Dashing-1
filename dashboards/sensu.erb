<% content_for :title do %>Sensu Status<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Clock"></div>
    <i class="icon-time icon-background"></i>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="sensu-status" data-view="Sensu" data-unordered="true" data-title="Sensu" data-moreinfo="Unacknowledged events"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="sensu-crit-list" data-view="List" data-title="Critical" />
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="sensu-warn-list" data-view="List" data-title="Warning" />
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1" style="z-index: 99999999">
      <div data-id="beer" data-view="Fridaybeer"></div>
    </li>
   <li data-row="1" data-col="1" data-sizex="1" data-sizey="1" data-title="External" />
    <div data-id="uptimerobot" data-view="List"></div>
   </li>
  </ul>
</div>
